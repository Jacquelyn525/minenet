{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Serilog.AspNetCore.RequestLoggingMiddleware": "Fatal",
        "Microsoft": "Fatal",
        "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "Fatal",
        "Microsoft.AspNetCore": "Warning",
        "System": "Fatal"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "c:\\code\\consol-energy\\logs\\log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "MineNet": {
    "MineNetPath": "C:\\MineNet",
    "DataPath": "Data",
    "HistoryPath": "History",
    "PageName": "Dev Test",
    "MinerInterval": 3
  },
  "AllowedGroups": [ "Group1", "Group2" ], // active directory, on-prem
  "AllowedHosts": "*"
}
