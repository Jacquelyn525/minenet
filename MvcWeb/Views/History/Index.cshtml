@model MvcWeb.Models.TagHistoryContext;

@{
    ViewData["Title"] = "MineNet Tag ID History";
}

<h1>Edit</h1>

<h4>Product</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="MinersOnShift" enctype="multipart/form-data">

            <div class="form-group">
            <label asp-for="TagIdListData" class="control-label">Miner:</label>
            <select>
                @foreach (var tag in Model.TagIdListData)
                {
                    <option asp-action="SelectedTag" asp-controller="History">@tag.FullName</option>

                }
            </select>
            </div>
            <div class="form-group">
                <input type="submit" value="Search" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
